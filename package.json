{
  "name": "@stagetimerio/google-secrets",
  "version": "0.6.0",
  "description": "Load secrets from Google Cloud Secret Manager into environment variables, similar to dotenv",
  "license": "ISC",
  "author": "Lukas Hermann <hey@lukashermann.dev>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./load": {
      "import": {
        "types": "./dist/load.d.mts",
        "default": "./dist/load.mjs"
      },
      "require": {
        "types": "./dist/load.d.cts",
        "default": "./dist/load.cjs"
      }
    }
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "google cloud",
    "secret manager",
    "environment variables",
    "dotenv",
    "secrets"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "tsdown": "^0.16.8",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  }
}
